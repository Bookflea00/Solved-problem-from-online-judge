#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t;
    cin>>t;
    int count=0;
    //int ans;
    while(t--)
    {
       double count=0; 
      string s;
      cin>>s;
      for(int i=0;i<s.length();i++)
      {
        count++;
      }
     // ans=0.01*count;
      //cout<<setprecision(2)<<0.01*count<<endl;
      printf("%.2lf\n",0.01*count);


    }
}
